<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <PackageId>OpenStackSwiftClient</PackageId>
    <PackageVersion>1.0.6</PackageVersion>
    <PackageTags>openstack swift netstandard2.0 tempurl</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>Francesco Benetton (me@frab.it)</Authors>
    <Company>Total Programmi</Company>
    <Description>OpenStack Swift netstandard2.0 Client, with support for TempUrl.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.Http.Polly" Version="6.0.9" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="6.0.0" />
    <PackageReference Include="Polly" Version="7.2.3" />
    <PackageReference Include="System.Net.Http.Json" Version="6.0.0" />
    <PackageReference Include="System.Text.Json" Version="6.0.6" />
  </ItemGroup>

</Project>
